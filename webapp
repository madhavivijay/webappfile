pipeline {
  agents any
    stages {
      stage ('build'){
        steps {
          git branch: 'main', url: 'https://github.com/madhavivijay/webappfile.git'
          sh 'mvn clean install'
    }
  }
      stage ('deploy') {
        steps {
          echo "this is to deploy"
        }
      }
    }
  }
