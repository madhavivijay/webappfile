pipeline {
  agent {label 'jslave'}
    stages {
      stage ('build'){
        steps {
          git 'https://github.com/madhavivijay/hello-world-webapp.git'
          sh 'mvn clean package'
    }
  }
      stage ('deploy') {
        steps {
          echo "this is to deploy"
        }
      }
    }
  }
